    "    \"firstTower\",\n",
    "    \"firstBaron\",\n",
    "    \"firstDragon\",\n",
    "    \"firstDragon\"\n",
    "]\n",
    "cat_transformer = Pipeline(steps = [\n",
    "    (\"onehot\", OneHotEncoder())\n",
    "])\n",
    "\n",
    "\n",
    "log_feat = ['towerKills', 'inhibitorKills', 'baronKills',\n",
