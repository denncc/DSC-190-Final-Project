   "source": [
    "### Model preprocessing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [],
   "source": [
    "X = df.drop([\"result\"], axis = 1)\n",
    "y = df.result"
